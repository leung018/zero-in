{
  "platforms": ["apple", "android", "web"],
  "apple": {
    "modules": ["AppBlockerModule"],
    "appExtensions": [
      {
        "targetName": "NotificationServiceExtension",
        "bundleIdentifier": "$(PRODUCT_BUNDLE_IDENTIFIER).notificationservice",
        "extensionPointIdentifier": "com.apple.usernotifications.service"
      }
    ]
  },
  "android": {
    "modules": ["expo.modules.appblocker.AppBlockerModule"]
  }
}
